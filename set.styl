@require 'vendor'

set-to()
  for n, i in arguments
    if i == 0
      v = n
    else
      if type v == string
        {n} s('%s', v)
      else
        {n} v

set-to-vendor()
  for n, i in arguments
    if i == 0
      v = n
    else
      if typeof v == string
        vendor(n, s('%s', v))
      else
        vendor(n, v)
